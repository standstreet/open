<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
        xmlns="http://www.standstreet.com/schema/error"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:beans="http://www.springframework.org/schema/beans"
        targetNamespace="http://www.standstreet.com/schema/error"
        elementFormDefault="qualified"
        attributeFormDefault="unqualified">
    <xsd:import namespace="http://www.springframework.org/schema/beans"/>
    <!-- error标签 -->
    <xsd:element name="error">
        <xsd:complexType>
            <xsd:complexContent>
                <xsd:extension base="beans:identifiedType">
                    <xsd:attribute name="code" type="xsd:int"/>
                    <xsd:attribute name="info" type="xsd:string"/>
                    <xsd:attribute name="des" type="xsd:string"/>
                </xsd:extension>
            </xsd:complexContent>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>